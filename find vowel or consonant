#include<stdio.h>
main()
{
char a;
printf("enter alphabet = ");
scanf("%c",&a);
if(a=='a' || a=='e' || a=='i' || a=='o' || a=='u' || a=='A' || a=='E' || a=='I' || a=='O' || a=='U')
	{
	printf("%c is vowel",a);
	}
else if((a>='a' && a<='z') || (a>='A' && a<='z'))
	{
	printf("%c is consonant",a);
	}
else
printf("WARNING = please enter alphabet");
}
